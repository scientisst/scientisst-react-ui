{
	"name": "@scientisst/react-ui",
	"version": "0.1.0",
	"repository": "https://github.com/scientisst/scientisst-react-ui.git",
	"author": "Rui Maciel <ruigouveiamaciel@gmail.com>",
	"contributors": [
		{
			"name": "Rui Maciel",
			"email": "ruigouveiamaciel@gmail.com",
			"url": "https://github.com/ruigouveiamaciel"
		}
	],
	"maintainers": [
		{
			"name": "Rui Maciel",
			"email": "ruigouveiamaciel@gmail.com",
			"url": "https://github.com/ruigouveiamaciel"
		}
	],
	"license": "MIT",
	"type": "module",
	"module": "dist/index.js",
	"scripts": {
		"dev": "next dev",
		"prebuild": "rm -rf dist/*",
		"build": "tsc --project tsconfig.build.json",
		"clean": "tsc-multi --clean",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"prettier": "prettier .",
		"prettier:fix": "prettier . --write"
	},
	"dependencies": {
		"clsx": "^1.2.1",
		"next": "^13.0.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"tailwindcss": "^3.2.1"
	},
	"devDependencies": {
		"@scientisst/eslint-config-next": "^1.0.1",
		"@scientisst/prettier-config-next": "^1.2.0",
		"@trivago/prettier-plugin-sort-imports": "^3.4.0",
		"@types/dom-serial": "^1.0.3",
		"@types/node": "^18.11.9",
		"@types/react": "^18.0.24",
		"eslint": "^8.26.0",
		"postcss": "^8.4.18",
		"prettier": "^2.7.1",
		"tsc-multi": "^0.6.1",
		"typescript": "^4.8.4"
	},
	"main": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./hooks": {
			"import": "./dist/hooks/index.js",
			"types": "./dist/hooks/index.d.ts"
		},
		"./tailwindcss-config": {
			"import": "./tailwind.config.cjs",
			"require": "./tailwind.config.cjs",
			"types": "./tailwind.config.d.cts"
		},
		"./dark-theme": {
			"import": "./dist/dark-theme/index.js",
			"types": "./dist/dark-theme/index.d.ts"
		},
		"./components/inputs": {
			"import": "./dist/components/inputs/index.js",
			"types": "./dist/components/inputs/index.d.ts"
		}
	},
	"typesVersions": {
		"*": {
			"hooks": [
				"dist/hooks/index.d.ts"
			],
			"tailwindcss-config": [
				"tailwind.config.d.cts"
			],
			"dark-theme": [
				"dist/dark-theme/index.d.ts"
			],
			"components/inputs": [
				"dist/components/inputs/index.d.ts"
			]
		}
	},
	"peerDependencies": {
		"react": ">= 18.2.0",
		"react-dom": ">= 18.2.0",
		"tailwindcss": ">= 3"
	}
}
