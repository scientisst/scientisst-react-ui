{
	"name": "scientisst-javascript-library",
	"version": "0.0.5",
	"repository": "https://github.com/scientisst/scientisst-javascript-library.git",
	"author": "Rui Maciel <ruigouveiamaciel@gmail.com>",
	"contributors": [
		{
			"name": "Rui Maciel",
			"email": "ruigouveiamaciel@gmail.com",
			"url": "https://github.com/ruigouveiamaciel"
		}
	],
	"maintainers": [
		{
			"name": "Rui Maciel",
			"email": "ruigouveiamaciel@gmail.com",
			"url": "https://github.com/ruigouveiamaciel"
		}
	],
	"license": "MIT",
	"type": "module",
	"scripts": {
		"prebuild": "yarn clean",
		"build": "tsc-multi",
		"clean": "tsc-multi --clean",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"prettier": "prettier .",
		"prettier:fix": "prettier . --write"
	},
	"dependencies": {
		"next": "^13.0.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"tailwindcss": "^3.2.1"
	},
	"devDependencies": {
		"@scientisst/eslint-config-next": "^1.0.1",
		"@scientisst/prettier-config-next": "^1.0.1",
		"@trivago/prettier-plugin-sort-imports": "^3.4.0",
		"@types/dom-serial": "^1.0.3",
		"@types/node": "^18.11.9",
		"@types/react": "^18.0.24",
		"eslint": "^8.26.0",
		"postcss": "^8.4.18",
		"prettier": "^2.7.1",
		"tsc-multi": "^0.6.1",
		"typescript": "^4.8.4"
	},
	"main": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs",
			"types": "./dist/index.d.ts"
		},
		"./hooks": {
			"import": "./dist/hooks/index.mjs",
			"require": "./dist/hooks/index.cjs",
			"types": "./dist/hooks/index.d.ts"
		},
		"./package.json": "./package.json"
	},
	"typesVersions": {
		"*": {
			"hooks": [
				"dist/hooks/index.d.ts"
			]
		}
	},
	"peerDependencies": {
		"react": ">= 18.2.0",
		"react-dom": ">= 18.2.0",
		"tailwindcss": ">= 3"
	}
}
