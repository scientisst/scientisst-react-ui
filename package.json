{
	"name": "@scientisst/react-ui",
	"version": "0.2.0",
	"repository": "https://github.com/scientisst/scientisst-react-ui.git",
	"author": "Rui Maciel <ruigouveiamaciel@gmail.com>",
	"contributors": [
		{
			"name": "Rui Maciel",
			"email": "ruigouveiamaciel@gmail.com",
			"url": "https://github.com/ruigouveiamaciel"
		}
	],
	"maintainers": [
		{
			"name": "Rui Maciel",
			"email": "ruigouveiamaciel@gmail.com",
			"url": "https://github.com/ruigouveiamaciel"
		}
	],
	"license": "MIT",
	"type": "module",
	"scripts": {
		"dev": "next dev",
		"prebuild": "rm -rf dist/*",
		"build": "tsc --project tsconfig.build.json",
		"prepublish": "yarn build",
		"clean": "tsc-multi --clean",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"prettier": "prettier .",
		"prettier:fix": "prettier . --write"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^6.2.0",
		"@fortawesome/free-brands-svg-icons": "^6.2.0",
		"@fortawesome/free-regular-svg-icons": "^6.2.0",
		"@fortawesome/free-solid-svg-icons": "^6.2.0",
		"@fortawesome/react-fontawesome": "^0.2.0",
		"clsx": "^1.2.1",
		"d3": "^7.6.1",
		"formik": "^2.2.9",
		"next": "^13.0.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"tailwindcss": "^3.2.1"
	},
	"devDependencies": {
		"@scientisst/eslint-config-next": "^1.0.1",
		"@scientisst/prettier-config-next": "^1.2.0",
		"@trivago/prettier-plugin-sort-imports": "^3.4.0",
		"@types/d3": "^7.4.0",
		"@types/dom-serial": "^1.0.3",
		"@types/node": "^18.11.9",
		"@types/react": "^18.0.24",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.26.0",
		"postcss": "^8.4.18",
		"prettier": "^2.7.1",
		"tsc-multi": "^0.6.1",
		"typescript": "^4.8.4"
	},
	"main": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		"./hooks": {
			"import": "./dist/hooks/index.js",
			"types": "./dist/hooks/index.d.ts"
		},
		"./tailwindcss-config": {
			"import": "./tailwind.config.cjs",
			"require": "./tailwind.config.cjs",
			"types": "./tailwind.config.d.cts"
		},
		"./dark-theme": {
			"import": "./dist/dark-theme/index.js",
			"types": "./dist/dark-theme/index.d.ts"
		},
		"./components/inputs": {
			"import": "./dist/components/inputs/index.js",
			"types": "./dist/components/inputs/index.d.ts"
		},
		"./components/inputs/common": {
			"import": "./dist/components/inputs/common/index.js",
			"types": "./dist/components/inputs/common/index.d.ts"
		},
		"./components/data-display": {
			"import": "./dist/components/data-display/index.js",
			"types": "./dist/components/data-display/index.d.ts"
		},
		"./components/navigation": {
			"import": "./dist/components/navigation/index.js",
			"types": "./dist/components/navigation/index.d.ts"
		},
		"./components/utils": {
			"import": "./dist/components/utils/index.js",
			"types": "./dist/components/utils/index.d.ts"
		}
	},
	"typesVersions": {
		"*": {
			"hooks": [
				"dist/hooks/index.d.ts"
			],
			"tailwindcss-config": [
				"tailwind.config.d.cts"
			],
			"dark-theme": [
				"dist/dark-theme/index.d.ts"
			],
			"components/inputs": [
				"dist/components/inputs/index.d.ts"
			],
			"components/inputs/common": [
				"dist/components/inputs/common/index.d.ts"
			],
			"components/data-display": [
				"dist/components/data-display/index.d.ts"
			],
			"components/navigation": [
				"dist/components/navigation/index.d.ts"
			],
			"components/utils": [
				"dist/components/utils/index.d.ts"
			]
		}
	},
	"peerDependencies": {
		"react": ">= 18.2.0",
		"react-dom": ">= 18.2.0",
		"tailwindcss": ">= 3.2.1",
		"next": ">= 13",
		"formik": ">= 2.2.9"
	}
}
